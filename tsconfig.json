{
  "compilerOptions": {
    "sourceMap": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "paths": {
      "components/*": [
        "src/components/*"
      ]
    },
    "strictNullChecks": false,
    "strict": false
  },
  "include": [
    "./index.tsx",
    "./webpack.config.js",
    "./declarations.d.ts"
  ]
}